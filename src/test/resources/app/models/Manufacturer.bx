@Entity
@Table ({
    "name" : "manufacturers"
})
class accessors=true {
    @id
    @ORMType("integer")
    @GeneratedValue({
        "strategy" : "increment"
    })
    property name="id";

    @Column
    @ORMType("string")
    property name="name";

    @Column
    @ORMType("string")
    property name="address";

    @OneToMany({
        "singularname" : "Vehicle",
        // "mappedBy" : "manufacturer",
        "class"    : "Vehicle",
        "fkcolumn" : "FK_manufacturer"
    })
    property name="vehicles";

}