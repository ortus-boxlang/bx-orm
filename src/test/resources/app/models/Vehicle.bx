class entityName="Vehicle" table="vehicles" persistent="true" {

    property 
        name="vin" 
        type="string" 
        fieldtype="id" 
        ormtype="string" 
        generator="assigned";
        
    property 
        name="make" 
        type="string";

    property 
        name="model" 
        type="string";
    

    property 
        name="manufacturer" 
        type="string"
        fieldtype="one-to-one"
        cfc="Manufacturer"
        fkcolumn="FK_manufacturer"
        lazy="false";

    function onPreInsert(){
        log.info( "Inserting new Vehicle: #getMake()# #getModel()#" );
    }
}