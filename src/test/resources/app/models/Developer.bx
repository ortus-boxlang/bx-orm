@Entity
@Table {
    "name" : "developers"
}
class accessors=true {
    @id
    @ORMType "integer"
    property name="id";

    @Column
    @ORMType "string"
    property name="name";

    @Column
    @ORMType "string"
    property name="role";

    @OneToOne{
        class     = "Vehicle",
        fieldtype = "one-to-one",
        cascade   = "none",
        lazy      = "false",
        fkColumn  = "vehicle_id",
    }
    property name="vehicle";
}