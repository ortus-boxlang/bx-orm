class{
	this.name = "BXORMTest";

	this.datasources={
		"TestDB" = {
			driver = "derby",
			database = "test",
			url = "jdbc:derby:memory:TestDB;create=true;",
			maxConnections = "3500"
		},
		"dsn2": {
			driver = "derby",
			database = "dsn2",
			url = "jdbc:derby:memory:dsn2;create=true;",
			maxConnections = "3500"
		}
	};
	this.datasource = "TestDB";
	this.ormSettings={
		"datasource" : "TestDB",
		"entityPaths": [ "src/test/resources/app/models" ],
		"saveMapping": "true",
		"logSQL"     : "true",
		"dialect"    : "DerbyTenSevenDialect"
	}
	this.ormEnabled = "true";
}
